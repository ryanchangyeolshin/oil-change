{
  "name": "oil-change",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "db:setup": "yarn db:create:dev && yarn db:create:test",
    "db:create:dev": "createdb --owner=ryanchangyeolshin oil_change_dev",
    "db:create:test": "createdb --owner=ryanchangyeolshin oil_change_test",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config ormconfig.js",
    "typeorm:g:entity": "npm run typeorm entity:create -n",
    "typeorm:migrate:generate": "npm run typeorm migration:generate -- -n",
    "typeorm:migrate:run": "npm run typeorm migration:run",
    "typeorm:create": "npm run typeorm migration:create -- -n",
    "typeorm:revert": "npm run typeorm migration:revert",
    "typeorm:schema:drop": "npm run typeorm schema:drop",
    "typeorm:schema:sync": "npm run typeorm schema:sync",
    "typeorm-seeding:config": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-seeding/dist/cli.js --configName ormconfig.js",
    "typeorm-seeding:run": "npm run typeorm-seeding:config seed"
  },
  "dependencies": {
    "@types/uuid": "^8.3.0",
    "dotenv": "^8.2.0",
    "next": "10.0.5",
    "pg": "^8.5.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "reflect-metadata": "^0.1.13",
    "swr": "^0.4.1",
    "typeorm": "^0.2.30",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-proposal-decorators": "^7.12.13",
    "@babel/preset-typescript": "^7.12.13",
    "@now/node": "^1.8.5",
    "@types/chance": "^1.1.1",
    "@types/express": "^4.17.11",
    "@types/faker": "^5.1.6",
    "@types/next": "^9.0.0",
    "@types/node": "^14.14.25",
    "@types/react": "^17.0.1",
    "@types/styled-components": "^5.1.7",
    "@types/styled-jsx": "^2.2.8",
    "@zeit/next-typescript": "^1.1.1",
    "babel-plugin-styled-components": "^1.12.0",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "typeorm-seeding": "^1.6.1",
    "typescript": "^4.1.3"
  }
}
